
@if (!string.IsNullOrEmpty(Convert.ToString(Session["MemberName"])))
{
    using (Html.BeginForm("LogOff", "MemberLogin", FormMethod.Post, new { id = "logoutForm", @class = "nav-item" }))
    {
        @Html.AntiForgeryToken()
        //<li>Hi:@Session["MemberName"]</li>
        <a href="javascript:document.getElementById('logoutForm').submit()" class="nav-link"><i class="fas fa-sign-in-alt"></i> </a>
    }
}


